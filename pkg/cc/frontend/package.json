{
  "name": "xcli-cc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "test": "pnpm test:unit",
    "test:unit": "vitest run --no-ui --config vitest.config.unit.ts --passWithNoTests",
    "test:unit:watch": "vitest --no-ui --config vitest.config.unit.ts",
    "test:storybook": "vitest run --no-ui --config vitest.config.ts",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-themes": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/builder-vite": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/browser": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-storybook": "^10.2.10",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "msw": "^2.12.10",
    "msw-storybook-addon": "^2.0.6",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^10.2.10",
    "tailwindcss": "^4.1.0",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.3.0",
    "vitest": "^4.0.18"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}